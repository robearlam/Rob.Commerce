<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <mvc>
      <precompilation>
        <assemblies>
          <assemblyIdentity name="Feature.RazorPrecompilation.Website" />
        </assemblies>
      </precompilation>
    </mvc>

    <pipelines>
      <initialize>
        <processor type="Sitecore.Speak.Pipelines.Initialize.PrecompileSpeakViews, Sitecore.Speak.Web">
          <patch:delete />
        </processor>
      </initialize>
    </pipelines>
    
  </sitecore>
</configuration>