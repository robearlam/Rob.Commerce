@using Sitecore.ContentTesting.Components
@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation

@*@Html.Sitecore().Controls().DataRepeater(Model.Rendering)*@

@model RenderingModel
@{
    var resolver = Html.Sitecore().Controls().GetParametersResolver(Model.Rendering);
    var rendering = new ContentTestingDataRepeater(resolver);
    var output = new HtmlString(rendering.Render());
}      
@output