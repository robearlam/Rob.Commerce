@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@using Sitecore.ContentTesting.Components
@model RenderingModel
@{
    var resolver = Html.Sitecore().Controls().GetParametersResolver(Model.Rendering);
    var control = new WrappingLink(resolver);
    var output = new HtmlString(control.Render());
}      
@output