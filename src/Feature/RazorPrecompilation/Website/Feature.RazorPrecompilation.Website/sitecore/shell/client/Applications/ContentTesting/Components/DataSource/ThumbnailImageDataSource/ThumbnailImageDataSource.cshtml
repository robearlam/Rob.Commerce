@using Sitecore.ContentTesting.Components.DataSource
@using Sitecore.Mvc
@using Sitecore.Mvc.Presentation
@model RenderingModel
@{
  
  var resolver = Html.Sitecore().Controls().GetParametersResolver(Model.Rendering);
  var rendering = new ThumbnailImageDataSource(resolver);
  var output = new HtmlString(rendering.Render());
}

@output
