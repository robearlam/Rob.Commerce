<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>Commerce Engine Product Import</Description>
    <Authors>Rob Earlam</Authors>
    <VersionPrefix>1.0.0</VersionPrefix>
    <TargetFramework>net471</TargetFramework>
    <PackageTags>$(PackageTags);Sitecore;Commerce;Plugin;Product;Import</PackageTags>
    <GenerateAssemblyTitleAttribute>false</GenerateAssemblyTitleAttribute>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Sitecore.Commerce.Core" Version="4.0.*" />
    <PackageReference Include="Sitecore.Commerce.Plugin.BusinessUsers" Version="4.0.*" />
    <PackageReference Include="Sitecore.Commerce.Plugin.Catalog" Version="4.0.*" />
    <PackageReference Include="Sitecore.Commerce.Plugin.Inventory" Version="4.0.*" />
    <PackageReference Include="Sitecore.Commerce.Plugin.Pricing" Version="4.0.*" />
    <PackageReference Include="Sitecore.Commerce.Plugin.Views" Version="4.0.*" />
  </ItemGroup>

</Project>